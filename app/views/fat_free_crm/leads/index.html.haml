= styles_for :lead

= render 'fat_free_crm/entities/title_bar'

= form_tag import_leads_path, multipart: true, class: "upload-excel" do
  = file_field_tag :file
  = submit_tag "Import"

.remote#create_lead{ hidden }

= render 'search'

.remote#options{ hidden }

.list#leads
  - if @leads.any?
    = render @leads
  - else
    = render "fat_free_crm/shared/empty"

#paginate= render "fat_free_crm/shared/paginate_with_per_page"
#export= render "fat_free_crm/shared/export"
